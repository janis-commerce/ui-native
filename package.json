{
	"name": "@janiscommerce/ui-native",
	"version": "0.0.1",
	"description": "components library for Janis app",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint .",
		"prepare": "husky install",
		"validate:code": "npm run lint -- --fix && npm t",
		"test:commit": "jest --colors --bail --findRelatedTests",
		"test:coverage": "jest --collectCoverage",
		"prestart": "yarn update-stories",
		"storybook-watcher": "sb-rn-watcher --config-path .ondevice",
		"update-stories": "sb-rn-get-stories --config-path .ondevice",
		"build-storybook": "build-storybook",
		"storybook:web": "start-storybook -p 6006",
		"storybook": "echo \"STORYBOOK=true\" > .env && yarn rnuc .env && yarn ios && echo \"STORYBOOK=false\" > .env && yarn rnuc .env",
		"storybook-web": "start-storybook -p 6006 --config-dir ./storybook-website",
		"storybook-web-build": "build-storybook --config-dir ./storybook-website --output-dir dist-storybook-website"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janis-commerce/ui-native.git"
	},
	"keywords": [
		"janis",
		"app",
		"ui"
	],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/janis-commerce/ui-native/issues"
	},
	"homepage": "https://github.com/janis-commerce/ui-native#readme",
	"peerDependencies": {
		"react": ">=^17.0.2 <=^18.2.0",
		"react-native": ">=^0.67.5 <=^0.71.5"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/preset-env": "^7.22.7",
		"@babel/runtime": "^7.22.6",
		"@react-native-community/eslint-config": "^2.0.0",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-controls": "^6.5.13",
		"@storybook/addon-ondevice-actions": "^6.0.1-beta.9",
		"@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.9",
		"@storybook/addon-ondevice-controls": "^6.0.1-beta.9",
		"@storybook/addon-ondevice-notes": "^6.0.1-beta.9",
		"@storybook/addon-react-native-web": "0.0.19",
		"@storybook/react": "^6.5.13",
		"@storybook/react-native": "^6.0.1-beta.9",
		"@testing-library/react-native": "^7.2.0",
		"@types/jest": "^25.2.3",
		"@types/react-native": "^0.63.2",
		"@types/react-test-renderer": "^16.9.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.32.0",
		"babel-jest": "^29.6.1",
		"babel-loader": "8.2.4",
		"babel-plugin-react-native-web": "^0.19.6",
		"eslint": "^7.14.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-import-resolver-babel-module": "^5.2.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-native": "^4.0.0",
		"husky": "^8.0.0",
		"jest": "^26.6.3",
		"jest-expo": "^41.0.0",
		"lint-staged": "^13.0.3",
		"metro-react-native-babel-preset": "^0.76.7",
		"react": "17.0.2",
		"react-dom": "^17.0.2",
		"react-native": "0.67.5",
		"react-native-svg-transformer": "^1.0.0",
		"react-test-renderer": "17.0.2",
		"setup-env": "^2.0.0",
		"typescript": "^3.8.3"
	},
	"resolutions": {
		"@types/react": "^16"
	},
	"lint-staged": {
		"*{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run test:commit"
		]
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.19.0",
		"@react-native-community/datetimepicker": "^7.4.0",
		"@react-native-community/slider": "^4.4.2",
		"react-native-safe-area-context": "^4.6.4",
		"react-native-svg": "12.1.1",
		"react-native-web": "^0.15.0"
	}
}
